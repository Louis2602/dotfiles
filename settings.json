{
	"editor.suggestSelection": "first",
	"explorer.confirmDelete": false,
	"auto-close-tag.disableOnLanguage": [],
	"files.exclude": {
		"**/.git": true,
		"**/.svn": true,
		"**/.hg": true,
		"**/CVS": true,
		"**/.DS_Store": true,
		"**/Thumbs.db": true,
		"**/node_modules": true
	},
	"files.associations": {
		"*html": "html",
		"*.css": "tailwindcss"
	},
	"editor.fontFamily": "Iosevka Nerd Font, JetBrainsMono Nerd Font, RobotoMono Nerd Font,  MesloLGS Nerd Font Mono, Cascadia Code NF, FiraCode NF",
	"editor.fontSize": 16,
	"editor.lineHeight": 1.5,
	"window.zoomLevel": 1.5,
	"terminal.integrated.fontSize": 16,
	"editor.hideCursorInOverviewRuler": true,
	"editor.wordWrapColumn": 150,
	"editor.fontWeight": "normal",
	"terminal.integrated.fontFamily": "Iosevka Nerd Font, JetBrainsMono Nerd Font, FiraCode NF, MesloLGS Nerd Font Mono, RobotoMono Nerd Font, CascadiaCode NF",
	"terminal.integrated.fontWeight": "normal",
	"workbench.editorAssociations": {
		"*.ipynb": "jupyter-notebook"
	},
	"liveServer.settings.donotShowInfoMsg": true,
	"security.workspace.trust.untrustedFiles": "open",
	"notebook.cellToolbarLocation": {
		"default": "right",
		"jupyter-notebook": "left"
	},
	"emmet.includeLanguages": {
		"javascript": "javascriptreact",
		"razor": "html",
		"vue-html": "html",
		"plaintext": "jade"
	},
	"liveServer.settings.donotVerifyTags": true,
	"notebook.lineNumbers": "on",
	"prettier.tabWidth": 4,
	"prettier.jsxSingleQuote": true,
	"prettier.singleQuote": true,
	"workbench.startupEditor": "none",
	"editor.quickSuggestionsDelay": 100,
	"editor.quickSuggestions": {
		"other": true,
		"comments": false,
		"strings": false
	},
	"editor.roundedSelection": false,
	"terminal.integrated.gpuAcceleration": "auto",
	"editor.bracketPairColorization.enabled": true,
	"editor.renderLineHighlight": "none",
	"editor.guides.bracketPairs": false,
	"files.trimTrailingWhitespace": true,
	"[markdown]": {
		"files.trimTrailingWhitespace": false
	},
	"terminal.integrated.profiles.windows": {
		"PowerShell": {
			"source": "PowerShell",
			"icon": "terminal-powershell"
		},
		"Command Prompt": {
			"path": [
				"${env:windir}\\Sysnative\\cmd.exe",
				"${env:windir}\\System32\\cmd.exe"
			],
			"args": [],
			"icon": "terminal-cmd"
		},
		"Git Bash": {
			"source": "Git Bash"
		}
	},
	"explorer.compactFolders": false,
	"terminal.integrated.defaultProfile.windows": "Git Bash",
	"javascript.updateImportsOnFileMove.enabled": "always",
	"terminal.integrated.scrollback": 50000,
	"prettier.useTabs": true,
	"window.title": "${dirty}${activeEditorMedium}${separator}${rootName}",
	"editor.acceptSuggestionOnCommitCharacter": false,
	"explorer.confirmDragAndDrop": false,
	"diffEditor.ignoreTrimWhitespace": false,
	"editor.minimap.renderCharacters": false,
	"typescript.updateImportsOnFileMove.enabled": "always",
	"githubPullRequests.pullBranch": "never",
	"reactSnippets.settings.prettierEnabled": true,
	"search.searchOnType": false,
	"workbench.tips.enabled": false,
	"editor.formatOnSave": true,
	"extensions.ignoreRecommendations": true,
	"git.openRepositoryInParentFolders": "never",
	"editor.accessibilitySupport": "off",
	"notebook.output.wordWrap": true,
	"chat.editor.wordWrap": "on",
	"workbench.iconTheme": "bearded-icons",
	"debug.terminal.clearBeforeReusing": true,
	"window.titleBarStyle": "custom",
	"tailwindCSS.emmetCompletions": true,
	"editor.glyphMargin": false,
	"editor.colorDecorators": false,
	"explorer.confirmPasteNative": false,
	"editor.linkedEditing": true,
	"update.mode": "manual",
	"editor.minimap.enabled": false,
	"breadcrumbs.enabled": false,
	"editor.fontLigatures": false,
	"editor.indentSize": "tabSize",
	"zenMode.hideLineNumbers": false,
	"editor.lineNumbers": "relative",
	"editor.inlineSuggest.enabled": false,
	"editor.cursorSurroundingLines": 100,
	"explorer.fileNesting.enabled": true,
	"explorer.fileNesting.patterns": {
		"*.ts": "${capture}.js",
		"*.js": "${capture}.js.map, ${capture}.min.js, ${capture}.d.ts",
		"*.jsx": "${capture}.js",
		"*.tsx": "${capture}.ts",
		"tsconfig.json": "tsconfig.*.json",
		"package.json": ".eslint*, prettier*, tsconfig*, vite*, pnpm-lock*, package-lock*, bun.lockb",
		"tailwind.config*": "tailwind.config*, postcss.config*",
		".env.local": ".env*",
		".env": ".env*",
		"*.sqlite": "${capture}.${extname}-*",
		"*.db": "${capture}.${extname}-*",
		"*.sqlite3": "${capture}.${extname}-*",
		"*.db3": "${capture}.${extname}-*",
		"*.sdb": "${capture}.${extname}-*",
		"*.s3db": "${capture}.${extname}-*"
	},
	"terminal.integrated.tabs.defaultIcon": "terminal-bash",
	"terminal.integrated.minimumContrastRatio": 1,
	"editor.suggest.preview": true,
	"eslint.validate": [
		"javascript",
		"javascriptreact",
		"typescript",
		"typescriptreact"
	],
	"editor.codeActionsOnSave": {
		"source.organizeImports": "never",
		"source.fixAll.eslint": "always"
	},
	"python.analysis.fixAll": ["source.unusedImports"],
	"[prisma]": {
		"editor.defaultFormatter": "Prisma.prisma"
	},

	// VIM MOTION SETUP
	"vim.leader": "<Space>",
	"vim.easymotion": true,
	"vim.hlsearch": true,
	"vim.sneak": true,
	"vim.foldfix": true,
	"vim.useSystemClipboard": true,
	"vim.useCtrlKeys": true,
	// use vim hjkl to navigate folders
	"workbench.list.defaultFindMode": "filter",
	// vim mode viw-y and p fix
	"vim.normalModeKeyBindingsNonRecursive": [
		// splits
		{ "before": ["s", "v"], "commands": [":vsplit"] },
		{ "before": ["s", "s"], "commands": [":split"] },
		{
			"before": ["leader", "r", "n"],
			"commands": ["editor.action.rename"]
		},
		// panes
		{
			"before": ["s", "h"],
			"commands": ["workbench.action.focusLeftGroup"]
		},
		{
			"before": ["s", "j"],
			"commands": ["workbench.action.focusBelowGroup"]
		},
		{
			"before": ["s", "k"],
			"commands": ["workbench.action.focusAboveGroup"]
		},
		{
			"before": ["s", "l"],
			"commands": ["workbench.action.focusRightGroup"]
		},
		{ "before": ["leader", "w"], "commands": [":w!"] },
		{ "before": ["leader", "q"], "commands": [":q!"] },
		{ "before": ["leader", "x"], "commands": [":x!"] },
		{
			"before": ["[", "d"],
			"commands": ["editor.action.marker.prev"]
		},
		{
			"before": ["]", "d"],
			"commands": ["editor.action.marker.next"]
		},
		{
			"before": ["leader", "c", "a"],
			"commands": ["editor.action.codeAction"]
		},
		{
			"before": ["leader", "f"],
			"commands": ["workbench.action.quickOpen"]
		},
		{
			"before": ["leader", ","],
			"commands": ["workbench.action.showAllEditors"]
		},
		{
			"before": ["leader", "p"],
			"commands": ["editor.action.formatDocument"]
		},
		{
			"before": ["K"],
			"commands": ["editor.action.showHover"]
		}
	],

	"vim.visualModeKeyBindingsNonRecursive": [
		{
			"before": ["p"],
			"commands": ["editor.action.clipboardPasteAction"]
		},
		{
			"before": ["J"],
			"commands": ["editor.action.moveLinesDownAction"]
		},
		{
			"before": ["K"],
			"commands": ["editor.action.moveLinesUpAction"]
		}
	],
	"vim.cursorStylePerMode.insert": "block",
	"vim.handleKeys": {
		"<C-a>": false,
		"<C-f>": false,
		"<C-v>": false,
		"<C-x>": false
	},
	"extensions.experimental.affinity": {
		"vscodevim.vim": 1
	},
	"workbench.tree.enableStickyScroll": true,
	"editor.stickyScroll.scrollWithEditor": false,
	"editor.stickyScroll.enabled": true,
	"gitlens.codeLens.includeSingleLineSymbols": true,
	"git.confirmSync": false,
	"errorLens.codeLensEnabled": true,
	"[typescriptreact]": {
		"editor.defaultFormatter": "esbenp.prettier-vscode"
	},
	"[typescript]": {
		"editor.defaultFormatter": "esbenp.prettier-vscode"
	},
	"files.autoSave": "afterDelay",
	"python.languageServer": "Pylance",
	"[python]": {
		"editor.defaultFormatter": "ms-python.black-formatter"
	},
	/*
	#1c1f20, #181818, #0078D4, #f8f8f8
	*/
	"workbench.colorCustomizations": {
		"editorCursor.background": "#181818",
		"editorCursor.foreground": "#ffdd33",
		"terminalCursor.foreground": "#ffdd33",
		"terminalCursor.background": "#181818",
		"tab.activeBorderTop": "#ffdd33",
		"tab.activeBorder": "#00000000"
	},
	"editor.tokenColorCustomizations": {
		"[Tokyo Night]": {
			"textMateRules": [
				{
					"scope": [
						"comment",
						"meta.var.expr storage.type",
						"keyword.control.flow",
						"keyword.control.return",
						"meta.directive.vue punctuation.separator.key-value.html",
						"meta.directive.vue entity.other.attribute-name.html",
						"tag.decorator.js entity.name.tag.js",
						"tag.decorator.js punctuation.definition.tag.js",
						"storage.modifier"
					],
					"settings": {
						"fontStyle": ""
					}
				}
			]
		}
	},
	"[jsonc]": {
		"editor.defaultFormatter": "esbenp.prettier-vscode"
	},
	"python.analysis.autoImportCompletions": true,
	"typescript.disableAutomaticTypeAcquisition": true,
	"workbench.editor.enablePreview": false,
	"typescript.preferences.includePackageJsonAutoImports": "on",
	"errorLens.gutterIconsEnabled": true,
	"editor.formatOnPaste": true,
	"eslint.format.enable": true,
	"workbench.editor.limit.value": 6,
	"sqlite3-editor.ui.tableHeaderMaxLines": 10,
	"workbench.layoutControl.enabled": false,
	"window.menuBarVisibility": "compact",
	"window.commandCenter": false,
	"oneDarkPro.italic": false,
	"eslint.workingDirectories": [
		{
			"mode": "auto"
		}
	],
	"[scss]": {
		"editor.defaultFormatter": "sibiraj-s.vscode-scss-formatter"
	},
	"editor.defaultFormatter": "esbenp.prettier-vscode",
	"editor.tabSize": 2,
	"prettier.configPath": ".prettierrc",
	"vs-code-prettier-eslint.prettierLast": true,
	"editor.wordWrap": "wordWrapColumn",
	"workbench.colorTheme": "Tsoding Emacs Gruber Minimal",
	"diffEditor.renderSideBySide": true
}
